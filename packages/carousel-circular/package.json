{
  "name": "@ddevkim/carousel-circular",
  "version": "0.1.0",
  "description": "A highly customizable 3D circular carousel component for React",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "pnpm run playground:dev",
    "playground:dev": "pnpm --filter @ddevkim/carousel-circular-playground dev",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "react",
    "carousel",
    "3d",
    "circular",
    "component",
    "ui",
    "typescript"
  ],
  "author": "ddevkim",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ddevkim/carousel-circular"
  },
  "bugs": {
    "url": "https://github.com/ddevkim/carousel-circular/issues"
  },
  "homepage": "https://github.com/ddevkim/carousel-circular#readme",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7"
  },
  "sideEffects": false
}
