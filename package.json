{
  "name": "@ddevkim/carousel-circular-3d",
  "version": "0.4.0",
  "description": "A luxury 3D circular carousel component for React with smooth animations, drag interactions, and premium visual effects",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.map",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "dist/**/*.css"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "pnpm run playground:dev",
    "playground:dev": "pnpm --filter @ddevkim/carousel-circular-3d-playground dev",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm run clean && pnpm run build && pnpm run type-check"
  },
  "keywords": [
    "react",
    "carousel",
    "3d",
    "circular",
    "component",
    "ui",
    "typescript",
    "animation",
    "drag",
    "interaction",
    "luxury",
    "smooth",
    "lightbox",
    "gallery",
    "slider",
    "swiper",
    "responsive",
    "touch",
    "momentum"
  ],
  "author": {
    "name": "ddevkim",
    "url": "https://github.com/ddevkim"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://carousel-circular.vercel.app",
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7"
  }
}
